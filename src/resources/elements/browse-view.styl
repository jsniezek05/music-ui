.browse
  color white
  margin-left 40px
  padding-right 40px
  position absolute
  height 120%
  width 750px
  overflow scroll
  .browse-nav
    height 35px
    width 740px
    border-bottom solid #454545 1px
    .nav-options
      height 40px
      font-size 18px
      margin-right 20px
      color #9d9d9d
      display inline
      &:hover
        color white
  .suggested
    position absolute
    margin-top 40px
    width 740px
    .suggested-header
      color white
      width 740px
    .suggestions
      width 740px
      height 400px
      border-top solid #454545 1px
      border-bottom solid #454545 1px
      .suggestion
        height 200px
        width 175px
        margin-right 20px
        background-color gray






